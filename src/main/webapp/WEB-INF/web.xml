<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 <!--
<display-name>feed</display-name>
<module-name>feed</module-name>
-->
<servlet>
	<servlet-name>MusicaServlet</servlet-name>
	<servlet-class>wagoner.musica.MusicaServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>QueryServlet</servlet-name>
	<servlet-class>wagoner.musica.QueryServlet</servlet-class>
</servlet>
 <servlet-mapping>
  <servlet-name>MusicaServlet</servlet-name>
  <url-pattern>/servlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>QueryServlet</servlet-name>
  <url-pattern>/queryservlet</url-pattern>
 </servlet-mapping>
<welcome-file-list>
<welcome-file>index.html</welcome-file>
</welcome-file-list>
</web-app>
